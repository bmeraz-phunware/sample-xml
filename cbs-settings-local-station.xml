<?xml version="1.0" encoding="UTF-8"?>
<atv>
    <head>
        <script src="http://cbs-staging.phunware.com/sample-xml/js/main.js"/>
    </head>
  <body>
    <optionDialog id="com.sample.cbs-liveTV-option-dialog">
      <header>
        <simpleHeader accessibilityLabel="Dialog with Options">
          <title>Local Station</title>
          <subtitle>Settings</subtitle>
        </simpleHeader>
      </header>
      <description>Please select which station you would like to watch</description>

      <menu>
        
        <sections>
          <menuSection>
            <items>
                
                <!--THESE ARE DYNAMICALLY POPULATED DEPENDING ON???-->
                <!--ONCE SELECTED, THIS CHOICE NEEDS TO BE LOCKED IN. IF USER GOES INTO LIVE TV SECTION, THIS SELECTION SHOULD TAKE HOLD-->
              <twoLineMenuItem id="list_0" accessibilityLabel="Option 1">
                  
                  <image src720="http://cbs-staging.phunware.com/sample-xml/images/720p/tv1.png" src1080="http://cbs-staging.phunware.com/sample-xml/images/1080p/tv1.png"/>
                  
                <label>Waco-Temple-Bryan | KBTX</label>
              </twoLineMenuItem>
              
              <twoLineMenuItem id="list_1" accessibilityLabel="Option 2">
                  
                  <image src720="http://cbs-staging.phunware.com/sample-xml/images/720p/tv1.png" src1080="http://cbs-staging.phunware.com/sample-xml/images/1080p/tv1.png"/>
                <label>Waco-Temple-Bryan | KTVR</label>
              </twoLineMenuItem>
            </items>
          </menuSection>
        </sections>
      </menu>
      
    </optionDialog>
  </body>
</atv>
