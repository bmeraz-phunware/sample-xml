<?xml version="1.0" encoding="UTF-8"?>
<atv>
  <body>
    <videoPlayer id="com.sample.video-player">
		<httpLiveStreamingVideoAsset id="ABCD1234" indefiniteDuration="false">
			<mediaURL>http://cbs-staging.phunware.com/sample-xml/sample-video/hls-sample/main_asset_master.m3u8</mediaURL>
			<title>Interstitials</title>
			<description>Live video stream with interstitials defined. Press up on the remote to see the interstitials while the video is playing. Override player callbacks in Javascript to prevent skipping during interstitials.</description>
			<image>http://cbs-staging.phunware.com/sample-xml/images/movie1.png</image>
			<eventGroup required="true">http://cbs-staging.phunware.com/sample-xml/sample-video/interstitials.xml</eventGroup>
			<upNextPresentationTime>80</upNextPresentationTime>
			<myMetadata>
        		<relatedPlaybackURL>http://cbs-staging.phunware.com/sample-xml/sample-video/hls-related-playback-with-up-next.xml</relatedPlaybackURL>
			</myMetadata>
		</httpLiveStreamingVideoAsset>
	</videoPlayer>
  </body>
</atv>
